generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Country {
  id   Int    @id @default(autoincrement())
  name String @unique
}

model Province {
  id   Int    @id @default(autoincrement())
  name String @unique
}

model User {
  id            Int       @id @default(autoincrement())
  role          String
  username      String    @unique
  password      String
  create_on     DateTime
  last_login    DateTime?
  refresh_token String?
}
